{
  "printWidth": 100,
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "singleQuote": true,
  "semi": true,
  "endOfLine": "auto",
  "importOrder": [
    "^react$",
    "^[a-z\\-/]*$",
    "(^@material-ui|components|providers)",
    "(^[src/]*$|^[./]|^[../])",
    ""
  ],
  "importOrderSeparation": true,
  "experimentalBabelParserPluginsList": ["js", "jsx"]
}
